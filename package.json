    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "prettier": "2.2.1",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "files": [
    "core/",
    "auth/",
